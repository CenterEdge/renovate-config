{
    "description": "This preset is used to combine github-actions updates into one pull request",
    "packageRules": [
        {
            "enabled": true,
            "groupName": "Github Actions",
            "matchUpdateTypes": ["major", "minor", "patch"]
            "matchManagers": [ "github-actions" ],
            "stabilityDays": 5
        }
    ]
}
